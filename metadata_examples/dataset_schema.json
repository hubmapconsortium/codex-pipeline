            "minimum": 0.0,
            "title": "The NominalMagnification schema",
            "description": "The magnification of the objective as specified by the manufacturer.",
            "default": 40,
            "examples": [
                40
            ]
        },
        "NumericalAperture": {
            "$id": "#/properties/NumericalAperture",
