        "TileHeight": {
            "$id": "#/properties/TileHeight",
            "type": "integer",
            "minimum": 1,
            "title": "The TileHeight schema",
            "description": "The size of a tile vertical direction in pixels.",
            "default": 1,
            "examples": [
                2048
            ]
