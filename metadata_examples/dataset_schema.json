        "NumCycles": {
            "$id": "#/properties/NumCycles",
            "type": "integer",
            "minimum": 1,
            "title": "The NumCycles schema",
            "description": "The number of cycles in the dataset.",
            "default": 1,
            "examples": [
                4
            ]
